{
  "name": "lyricova-jukebox",
  "version": "2.0.0",
  "description": "A public jukebox and a music library manager for Project Lyricova.",
  "scripts": {
    "start": "yarn run serve",
    "build": "yarn run build-ts && yarn run lint && yarn run build-next",
    "serve": "ts-node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -n node,tscr -c green,blue yarn:watch-node yarn:watch-ts",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "build-next": "next build",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "yarn run build && yarn run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "yarn run watch serve-debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueset/project-lyricova.git"
  },
  "keywords": [
    "Lyricova",
    "Project",
    "Lyricova",
    "music",
    "library",
    "jukebox"
  ],
  "author": "Eana Hufwe",
  "license": "AGPL-3.0-or-later",
  "bugs": {
