{
  "name": "lyricova-jukebox",
  "version": "2.0.0",
  "description": "A public jukebox and a music library manager for Project Lyricova.",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-sass && npm run build-ts && npm run lint && npm run copy-static-assets",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueset/project-lyricova.git"
  },
  "keywords": [
    "Lyricova",
    "Project",
    "Lyricova",
    "music",
    "library",
    "jukebox"
  ],
  "author": "Eana Hufwe",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/blueset/project-lyricova/issues"
  },
  "homepage": "https://github.com/blueset/project-lyricova#readme",
  "dependencies": {
    "@types/pinyin": "^2.8.1",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "async": "^3.1.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^6.0.0",
    "connect-typeorm": "^1.1.3",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.0",
    "express-validator": "^6.3.1",
    "ffmetadata": "^1.6.0",
    "ffprobe-client": "^1.1.6",
    "glob": "^7.1.6",
    "hasha": "^5.1.0",
    "lodash": "^4.17.15",
    "lyrics-kit": "1.0.0",
    "lusca": "^1.6.1",
    "mecab-async": "^0.1.2",
    "mysql": "^2.17.1",
    "mysql2": "^2.1.0",
    "nodejieba": "^2.4.0",
    "p-limit": "^2.2.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pinyin": "^2.9.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "sequelize": "^5.21.3",
    "sequelize-typescript": "^1.1.0",
    "typeorm": "^0.2.22",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/async": "^3.0.3",
    "@types/bluebird": "^3.5.29",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.17.2",
    "@types/express-flash": "0.0.1",
    "@types/express-session": "^1.15.16",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.6",
    "@types/passport": "^1.0.2",
    "@types/validator": "^12.0.1",
    "concurrently": "^5.0.2",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  }
}